witchery.werewolf.level3begin=La voce intona, "RINFORZA I TUOI ARTIGLI, LACERA LA CARNE DI %s DELLE PREDE PIU' DEBOLI, PIAZZA GLI AGNELLI DI FRONTE A ME!"
witchery.werewolf.level3progress=La voce echeggia, "UCCIDINE %s E PORTAMI LA LORO CARNE, HAI BISOGNO DI ALTRI %s!"
witchery.werewolf.level3complete=La voce urla, "CARNE STRAPPATA DAI PIU' DEBOLI, ARTIGLI COME IL METALLO, PER STRAPPARE TERRA E CARNE, LA FAME DELLA BESTIA E' SEMPRE IN BALLO!" 

witchery.werewolf.level4begin=La voce tuona, "LE ZANNE DELL'ALFA SI AFFILANO, STRAPPA LA GOLA DI %s LUPI MINORI E PORTAMI LE LORO LINGUE!"
witchery.werewolf.level4progress=La voce echeggia, "DOMINANE %s, %s RIMANGONO!"
witchery.werewolf.level4complete=La voce urla, "IL BRANCO SI AFFAMA, LE ZANNE PRANZANO SULL'AMMAZZATO, UN NUOVO ALFA NASCE, DAGLI UCCISI SARAI SAZIATO!" 

witchery.werewolf.level5begin=La voce tuona, "IL LUPO E L'UOMO DEVONO DIVENTARE UNO, UCCIDI IL CACCIATORE CORNUTO! EVOCALO CON QUESTO CORNO E DAGLI IL COLPO FINALE!"
witchery.werewolf.level5progress=La voce echeggia, "IL CACCIATORE NON E' SCONFITTO! EVOCALO CON IL CORNO E DAGLI IL COLPO FINALE!"
witchery.werewolf.level5complete=La voce urla, "UN POTENTE CAMPIONE SCONFITTO, TU SEI STATO A FARLO, LA BESTIA E' RISVEGLIATA, IL COLPO SU 2 O 4 GAMBE POTRAI DARLO!" 

witchery.werewolf.level6begin=La voce ringhia, "ALLENA POTENTI GAMBE PER SALTARE SUI NEMICI, BALZA SU %s MOSTRI, E UCCIDI DALL'ARIA!"
witchery.werewolf.level6progress=La voce tuona, "UCCIDI %s DALL'ARIA, %s RIMANGONO!"
witchery.werewolf.level6complete=La voce proclama, "MORTE DA SOPRA, COLPISCI COME IL LAMPO, CORRI QUANDO ATTACCHI, E NON CI SARA' SCAMPO!"

witchery.werewolf.level7begin=La voce urla, "GLI ALTRI TI DEVONO TEMERE, VIAGGIA DI NOTTE, GUARDA AL CIELO IN %s POSTI E FA SENTIRE IL TUO ULULATO!"
witchery.werewolf.level7progress=La voce intona, "ULULA AL CIELO NOTTURNO IN %s POSTI, %s RIMANGONO!"
witchery.werewolf.level7complete=La voce ringhia, "LA TUA VOCE E' SENTITA, AL SENTIRLA TREMANO, COME ULULA IL LUPO, I NEMICI SI ARRESTANO!"

witchery.werewolf.level8begin=La voce echeggia, "IMPADRONISCITI DEL BRANCO! COME LUPO, TROVA E PIEGA %s LUPI ALLA TUA VOLONTA'!"
witchery.werewolf.level8progress=La voce intona, "DA UN BRANCO DI %s LUPI, DEVI DOMINARNE ANCORA %s!"
witchery.werewolf.level8complete=La voce tuona, "L'ALFA DEL BRANCO, IL DOMINIO NON PIU' UN SOGNO, ULULA AL CIELO, SE DEI TUOI SERVI C'E' BISOGNO."

witchery.werewolf.level9begin=La voce urla, "ARTIGLI COME COLTELLI, COME MANNARO DEVI CACCIARE, UCCIDI %s DEI MAIALI CHE RUGGISCONO NEL NETHER!"
witchery.werewolf.level9progress=La voce intona, "COME MANNARO, CACCIA %s MAIALI DEL NETHER, %s DEVONO ESSERE TROVATI!"
witchery.werewolf.level9complete=La voce proclama, "I NEMICI INFERNALI SONO SCONFITTI, GLI ARTIGLI AGUZZI, L'ARMATURA, COME CARTA, ORA FARAI A PEZZI!"

witchery.werewolf.level10begin=La voce urla, "PRENDI LA VITA DI UN ALTRO, DA VILLAGGIO O AMICO, POI TI VERRA' DATO IL POTERE, DI SPARGERE LA MIA BENEDIZIONE!"
witchery.werewolf.level10progress=La voce echeggia, "UCCIDI DA UN VILLAGGIO, O UNO COME TE!"
witchery.werewolf.level10complete=La voce esclama, "HAI COMPLETATO LA MIA MISSIONE, ORA SPARGI LA BENEDIZIONE!"

